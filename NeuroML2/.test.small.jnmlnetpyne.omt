  spike_rate_exc_in:
    observables:
      spike rate:
        spiketimes file: 
          path: expoisson.spikes
          format: TIME_ID
          scaling: 1
          ids: 0
        tolerance: 0.0021004200850149573
  spike_rate_inh_in:
    observables:
      spike rate:
        spiketimes file: 
          path: inpoisson.spikes
          format: TIME_ID
          scaling: 1
          ids: 0
        tolerance: 0.024629486918773418