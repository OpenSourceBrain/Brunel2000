# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_Sim_BrunelFromPyNN.xml
engine: jNeuroML
mep: ../PyNN/.test.small.mep
experiments:

  spike_rate_e:
    observables:
      spike rate:
        spiketimes file: 
          path: E_net.spikes
          format: TIME_ID
          ids: 0
        tolerance: 0.02169920745362061
  spike_rate_i:
    observables:
      spike rate:
        spiketimes file: 
          path: I_net.spikes
          format: TIME_ID
          ids: 0
        tolerance: 0.0006482679636328364
        
  spike_rate_exc_in:
    observables:
      spike rate:
        spiketimes file: 
          path: expoisson.spikes
          format: TIME_ID
          scaling: 1
          ids: 0
        tolerance: 0.002275284410551285
  spike_rate_inh_in:
    observables:
      spike rate:
        spiketimes file: 
          path: inpoisson.spikes
          format: TIME_ID
          scaling: 1
          ids: 0
        tolerance: 0.00429875594954221