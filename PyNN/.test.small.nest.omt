# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: brunel_to_neuroml.py 
engine: PyNN_Nest
mep: .test.small.mep
experiments:

  spike_rate_e:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-nest-E_net-0.gdf
          format: TIME_ID
          id: 0
        tolerance: 0.00
  spike_rate_i:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-nest-I_net-0.gdf
          format: TIME_ID
          id: 0
        tolerance: 0.00
        
  spike_rate_exc_in:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-nest-expoisson-0.gdf
          format: TIME_ID
          id: 0
        tolerance: 0.00
  spike_rate_inh_in:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-nest-inpoisson-0.gdf
          format: TIME_ID
          id: 0
        tolerance: 0.00