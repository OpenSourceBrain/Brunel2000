# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: brunel_to_neuroml.py 
engine: PyNN_NEURON
mep: .test.small.mep
experiments:

  spike_rate_e:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-neuron-E_net-0.gdf
          format: TIME_ID
          scaling: 0.001
          id: 0
        tolerance: 0.003091149274902911
  spike_rate_i:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-neuron-I_net-0.gdf
          format: TIME_ID
          scaling: 0.001
          id: 0
        tolerance: 0.008231242178950186
        
  spike_rate_exc_in:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-neuron-expoisson-0.gdf
          format: TIME_ID
          scaling: 0.001
          id: 0
        tolerance: 0.003853429031591052
  spike_rate_inh_in:
    observables:
      spike rate:
        spiketimes file: 
          path: brunel-PyNN-neuron-inpoisson-0.gdf
          format: TIME_ID
          scaling: 0.001
          id: 0
        tolerance: 0.00806801623469927